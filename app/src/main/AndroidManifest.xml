<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.experiment.lenovo.accountingsoftware">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoTitle">
        <activity android:name=".billinfopage.BillInfoActivity"></activity>
        <activity android:name=".accuntingpage.AccountingActivity" android:windowSoftInputMode="adjustPan"></activity>
        <activity android:name=".MainActivity" ></activity>
        <activity android:name=".LoginPage.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET" />

</manifest>